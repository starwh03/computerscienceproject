<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="111.0" prefWidth="202.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.getname">
   <center>
      <TextField fx:id="Textfield" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <BorderPane prefHeight="30.0" prefWidth="202.0">
         <bottom>
		      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Playlist Name" BorderPane.alignment="CENTER" />
	     </bottom>
         <top>
            <BorderPane fx:id="titlebar" prefHeight="21.0" prefWidth="202.0" BorderPane.alignment="CENTER">
               <right>
                  <Button fx:id="close" mnemonicParsing="false" prefHeight="23.0" prefWidth="30.0" text="Button" BorderPane.alignment="CENTER" />
               </right>
            </BorderPane>
         </top>
	  </BorderPane>
   </top>
   <bottom>
      <Button fx:id="OK" mnemonicParsing="false" text="OK" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <left>
      <VBox prefHeight="34.0" prefWidth="27.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <VBox prefHeight="34.0" prefWidth="27.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
