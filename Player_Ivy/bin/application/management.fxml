<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="500.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Management">
   <left>
      <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="listing" prefHeight="350.0" prefWidth="100.0" />
         </content>
         <BorderPane.margin>
            <Insets left="10.0" />
         </BorderPane.margin>
      </ScrollPane>
   </left>
   <right>
      <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="listall" prefHeight="350.0" prefWidth="100.0" />
         </content>
         <BorderPane.margin>
            <Insets right="10.0" />
         </BorderPane.margin>
      </ScrollPane>
   </right>
   <top>
      <HBox fx:id="titlebar" prefHeight="54.0" prefWidth="850.0" BorderPane.alignment="CENTER">
         <children>
            <BorderPane prefHeight="54.0" prefWidth="850.0">
               <left>
                  <BorderPane prefHeight="54.0" prefWidth="420.0" BorderPane.alignment="CENTER">
                     <center>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" BorderPane.alignment="CENTER">
                           <font>
                              <Font name="Arial" size="22.0" />
                           </font>
                        </Text>
                     </center>
                  </BorderPane>
               </left>
               <right>
                  <BorderPane prefHeight="54.0" prefWidth="420.0" BorderPane.alignment="CENTER">
                     <center>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="All Contents" BorderPane.alignment="CENTER">
                           <font>
                              <Font name="Arial" size="22.0" />
                           </font>
                        </Text>
                     </center>
                  </BorderPane>
               </right>
            </BorderPane>
         </children>
      </HBox>
   </top>
   <center>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="&lt;&lt;" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <BorderPane prefHeight="41.0" prefWidth="850.0" BorderPane.alignment="CENTER">
         <left>
            <BorderPane prefHeight="41.0" prefWidth="420.0" BorderPane.alignment="CENTER">
               <right>
                  <Button fx:id="Cancel" prefWidth="70.0" mnemonicParsing="false" text="Cancel" BorderPane.alignment="CENTER" />
               </right>
            </BorderPane>
         </left>
         <right>
            <BorderPane prefHeight="41.0" prefWidth="420.0" BorderPane.alignment="CENTER">
               <left>
                  <Button fx:id="Apply" prefWidth="70.0" mnemonicParsing="false" text="Apply" BorderPane.alignment="CENTER" />
               </left>
            </BorderPane>
         </right>
      </BorderPane>
   </bottom>
</BorderPane>
